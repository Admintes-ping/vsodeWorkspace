						"buttons": [{
							"fontSrc": "/static/tianjia.ttf", // 自定义字体图标的路径
							"text": "\ue620", // 字体图标的 Unicode
							"fontSize": "28", // 字体大小
							"color": "#ffff", // 字体颜色
							"width": "42px", // 按钮宽度
							"background": "rgba(0,0,0,0)" // 按钮背景色为透明
						}]
					}
				},
				"enablePullDownRefresh": false
			}

